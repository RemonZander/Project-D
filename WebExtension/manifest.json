{
    "manifest_version": 3,
    "name":    "bol.com image search",
    "version": "0.1",

    "background": {
        "service_worker": "searchImage.js"
    },

    "permissions": [
        "contextMenus",
        "activeTab"
    ],

    "host_permissions": [
        "https://*/",
        "http://*/"
    ]
}